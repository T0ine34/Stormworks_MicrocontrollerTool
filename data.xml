<!-- link between elements -->
<!-- we have 1 on/off input, linked on both inputs of a and gate -->
<?xml version="1.0" encoding="UTF-8"?>
<microprocessor name="Microcontroller" description="No description set." width="2" length="2" id_counter="96" id_counter_node="2">
	<nodes>
		<n id="2" component_id="95">
			<node label="Input" mode="1" description="The input signal to be processed."/>
		</n>
	</nodes>
	<group>
		<data>
			<inputs/>
			<outputs/>
		</data>
		<components>
			<c type="1">
				<object id="96">
					<pos x="2" y="-0.25"/>
					<in1 component_id="95"/>
					<in2 component_id="95"/>
				</object>
			</c>
		</components>
		<components_bridge>
			<c>
				<object id="95"/>
			</c>
		</components_bridge>
		<groups/>
		<component_states>
			<c0 id="96">
				<pos x="2" y="-0.25"/>
				<in1 component_id="95"/>
			</c0>
		</component_states>
		<component_bridge_states>
			<c0 id="95"/>
		</component_bridge_states>
		<group_states/>
	</group>
</microprocessor>

